<toast activationType="protocol" launch="https://unsplash.com/">  <visual>    <binding template="ToastGeneric">      <text>New wallpaper</text>      <text>Photo downloaded from Unsplash</text>      <image src="https://via.placeholder.com/360x200?text=Backsplashed"/>    </binding>  </visual>  <actions>    <action      content="Save"      activationType="background"      arguments="action=saveCurrentWallpaper&amp;photoId="/>    <action      content="Lock"      activationType="background"      arguments="action=disableAutoUpdate"/>    <action      content="Next"      activationType="background"      arguments="action=getNextWallpaper"/>  </actions></toast>